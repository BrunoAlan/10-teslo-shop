{
  "expo": {
    "name": "teslo",
    "slug": "teslo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/no-product-image.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alanbruno.teslo"
    },
    "android": {
      "adaptiveIcon": {
        "icon": "./src/assets/no-product-image.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.alanbruno.teslo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "icon": "./src/assets/no-product-image.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you take photos.",
          "microphonePermission": "The app accesses your microphone to let you record videos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
